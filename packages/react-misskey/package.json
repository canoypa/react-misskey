{
  "name": "react-misskey",
  "version": "0.1.0",
  "description": "",
  "license": "AGPL-3.0-only",
  "repository": "https://github.com/canoypa/react-misskey",
  "bugs": {
    "url": "https://github.com/canoypa/react-misskey/issues"
  },
  "exports": {
    ".": {
      "react-server": "./dist/index.server.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*.{js,d.ts,css}"
  ],
  "type": "module",
  "scripts": {
    "build": "pnpm run build:swc && pnpm run build:type",
    "build:swc": "swc src -d dist --copy-files",
    "build:type": "tsc --project tsconfig.build.json",
    "watch:swc": "pnpm run build:swc -w",
    "watch:type": "pnpm run build:type -w",
    "dev": "run-p watch:*",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "mfm-js": "^0.24.0",
    "punycode": "^2.3.1",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/addon-onboarding": "^8.1.6",
    "@storybook/blocks": "^8.1.6",
    "@storybook/react": "^8.1.6",
    "@storybook/react-vite": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.25",
    "@types/punycode": "^2.1.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.1.6",
    "typescript": "^5.4.5",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.4.12"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  }
}
