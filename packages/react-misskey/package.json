{
  "name": "react-misskey",
  "version": "0.1.0",
  "description": "",
  "license": "AGPL-3.0-only",
  "repository": "https://github.com/canoypa/react-misskey",
  "bugs": {
    "url": "https://github.com/canoypa/react-misskey/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*.{js,d.ts,css}"
  ],
  "type": "module",
  "scripts": {
    "build": "pnpm run build:swc && pnpm run build:type",
    "build:swc": "swc src -d dist --copy-files",
    "build:type": "tsc --project tsconfig.build.json",
    "watch:swc": "pnpm run build:swc -w",
    "watch:type": "pnpm run build:type -w",
    "dev": "run-p watch:*",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "mfm-js": "^0.24.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.6.8",
    "@storybook/addon-interactions": "7.6.8",
    "@storybook/addon-links": "7.6.8",
    "@storybook/addon-onboarding": "1.0.10",
    "@storybook/blocks": "7.6.8",
    "@storybook/react": "7.6.8",
    "@storybook/react-vite": "7.6.8",
    "@storybook/test": "7.6.8",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.103",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "7.6.8",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  }
}
